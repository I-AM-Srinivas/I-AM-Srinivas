#include<stdio.h>
int main()
{
        int a,b,c,d,e,f,g;
        a = 20 , b = 6 , c = a + b , d = a - b , e = a * b , f = a / b , g = a % b;
        printf("Value of 20 and 6 is %d \n ",c);
        printf("Value of 20 and 6 is %d \n",d);
        printf("Value of 20 and 6 is %d \n ",e);
        printf("Value of 20 and 6 is %d \n ",f);
        return 0;
}
